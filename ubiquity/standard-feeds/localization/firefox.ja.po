msgid "count tabs.names"
msgstr "タブ数カウント"

msgid "count tabs.contributors"
msgstr "marsf"

msgid "count tabs.help"
msgstr ""

msgid "count tabs.description"
msgstr "開いているタブの数を数えます。"

msgctxt "count tabs._count"
msgid "<div class="count-tabs"><b>${count}</b> tab{if count > 1}s{/if} {if html}matching <b>${html}</b>{else}total{/if}</b>.</div>"
msgstr "<div class="count-tabs"><b>${count}</b> のタブ{if html}がマッチしました: <b>${html}</b>{else}が開いています。{/if}</b>.</div>"


