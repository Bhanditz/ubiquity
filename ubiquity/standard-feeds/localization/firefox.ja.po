msgstr "<pre class=\"bookmarklet\" style=\"white-space:pre-wrap\">${data}</pre>"


#. undo-closed-tabs command:
msgid "undo closed tabs.names"
msgstr "タブ復元|閉じたタブを復元|閉じたタブを開く|uct"

msgid "undo closed tabs.contributors"
msgstr "marsf"

msgid "undo closed tabs.help"
msgstr "<ul style=\"list-style-image:none\">\n
    <li>アクセスキーを入力するかクリックしてください。</li>\n
    <li>フィルタを入力して実行すると、すべてのタブを復元します。</li>\n
    </ul>"

msgid "undo closed tabs.description"
msgstr "最近閉じたタブを開きます。"

msgctxt "undo closed tabs.preview"
msgid "No closed tabs."
msgstr "閉じたタブはありません。"

msgctxt "undo closed tabs.preview"
msgid "No matched tabs."
msgstr "マッチするタブはありません。"


