                <!-- A page describing what is new in this updated
                     version -->
                <em:updateInfoURL>https://people.mozilla.com/~avarma/ubiquity-0.1.1.xhtml</em:updateInfoURL>
              </RDF:Description>
            </em:targetApplication>
          </RDF:Description>
        </RDF:li>

      </RDF:Seq>
    </em:updates>
  </RDF:Description>
</RDF:RDF>
